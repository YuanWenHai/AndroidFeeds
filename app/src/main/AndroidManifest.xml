<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.will.androidfeeds">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:name="com.will.androidfeeds.base.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.will.androidfeeds.MainActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".csdn.content.CsdnContentActivity"
            android:theme="@style/CsdnTheme"/>
        <activity android:name=".hukai.content.HKContentActivity"
            android:theme="@style/HuKaiTheme"/>
        <activity android:name=".publicAccount.list.PAContentList"
            android:theme="@style/PublicAccountTheme"/>
        <activity android:name=".publicAccount.content.PAContentActivity"
            android:theme="@style/PublicAccountTheme"/>
        <activity android:name=".droidyue.content.DroidYueContentActivity"
            android:theme="@style/DroidYueTheme"/>
        <activity android:name=".stylingAndroid.content.StylingAndroidContentActivity"
            android:theme="@style/StylingAndroidTheme"/>
        <activity android:name=".danLew.content.DanLewContentActivity"
            android:theme="@style/DanLewTheme"/>
        <activity android:name=".favorite.content.FavoriteContentActivity"
            android:theme="@style/FavoriteTheme"/>
    </application>

</manifest>